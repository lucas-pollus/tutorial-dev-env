{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "ctrl+w"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "horizontal"
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "keys": "ctrl+tab"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "keys": "ctrl+shift+tab"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        }
    ],
    "alwaysShowTabs": true,
    "defaultProfile": "{ba50f801-2d96-4517-a737-575f32f0fb61}",
    "initialCols": 150,
    "initialRows": 45,
